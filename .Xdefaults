
!urxvt.termName:  rxvt-256color
Rxvt.loginShell:         true
Rxvt.scrollBar:         false
Rxvt.secondaryScroll:    true
Rxvt.saveLines:         65535
Rxvt.cursorBlink:       false
Rxvt.urgentOnBell:       true
!urxvt.borderLess:         true
!!urxvt.borderWidth:          1
!!urxvt.internalBorder:       0
!!
!! Extensions
Rxvt*perl-lib:         /usr/lib/urxvt/perl/
Rxvt*perl-ext-common:  default,matcher,searchable-scrollback
Rxvt*urlLauncher:      /usr/local/bin/firefox
Rxvt*matcher.button:   1
! So we catch even nasty URLs
Rxvt.cutchars:         `()'*<>[]{|}"
Rxvt*cutchars:         `"()'*<>[]{|}
!
! Appearance
Rxvt.font:    -*-terminus-*-r-*-*-14-*-*-*-*-*-*-u
Rxvt.boldFont: -*-terminus-bold-r-*-*-14-*-*-*-*-*-*-u
!
! Cursor
rxvt.cursorColor: #DCDCCC
!
! Pseudo Transparency
Rxvt.shading:     30
Rxvt.transparent: true
!! 
! Zenburn color-scheme
Rxvt*background:  #333333
Rxvt*foreground:  #ddddcc
! black + red
!urxvt*color0:     #3f3f3f
Rxvt*color0:      #113322
Rxvt*color1:      #884444
! green + yellow
Rxvt*color2:      #55cc77
Rxvt*color3:      #ffbb77
! blue + purple
Rxvt*color4:      #445588
Rxvt*color5:      #ee77bb
! cyan + white
Rxvt*color6:      #77ccee
Rxvt*color7:      #ddddcc
! bright-black + bright-red
Rxvt*color8:      #66aa77
Rxvt*color9:      #ee9999
! bright-green + bright-yellow
Rxvt*color10:     #aadd88
Rxvt*color11:     #ffdd88
! bright-blue + bright-purple
Rxvt*color12:     #88aaff
Rxvt*color13:     #ff88cc
! bright-cyan + bright-white
Rxvt*color14:     #88eeff
Rxvt*color15:     #ffffee

